@*
    
@Model IEnumerable<Account>


For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}

<h1>Account</h1>

<span>@ViewBag.UserName</span>


<table>
    <thead>
        <tr>
            <th>Id</th>
            <th>Balance</th>
        </tr>
    </thead>
    <tbody>
            @{
                foreach (var m in Model)
                {
                    <tr>
                    
                        <td>@m.Id</td>
                        <td>@m.Balance</td>

                    </tr>
                }

        }
    </tbody>
</table>

<br>
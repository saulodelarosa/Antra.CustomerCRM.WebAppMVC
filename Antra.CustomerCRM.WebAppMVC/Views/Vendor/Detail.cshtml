@model VendorModel
<div class="row">
    <div class="col-5">
        <img src="https://img.lovepik.com/free-png/20211211/lovepik-vendor-selling-fruit-png-image_401493255_wh1200.png" width="250px" height="250px"/>
    </div>
    <div class="col-7">
        <table class="table table-bordered table-striped">
            <tr> 
                <td>Name</td>
                <td>@Model.Name</td>
            </tr>
             <tr> 
                <td>City</td>
                <td>@Model.City</td>
            </tr>
            <tr> 
                <td>Country</td>
                <td>@Model.Country</td>
            </tr>
            <tr> 
                <td>Mobile</td>
                <td>@Model.Mobile</td>
            </tr>
            <tr> 
                <td>EmailId</td>
                <td>@Model.EmailId</td>
            </tr>
            <tr> 
                <td>IsActive</td>
                <td>@Model.IsActive</td>
            </tr>
           
        </table>
    </div>
</div>